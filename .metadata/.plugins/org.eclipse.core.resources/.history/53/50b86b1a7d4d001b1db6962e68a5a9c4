package com.example.hi;

public class P01 {

	public static void main(String[] args) {
		//21-01-03
		//참고한 사이트 (https://raccoonjy.tistory.com/7?category=744507)

		System.out.print("안녕");
		System.out.println("반갑다");
		System.out.println("오늘은" + 1 + "번째 연습시간임");
		System.out.println("3 + 4 = " + 3 + 4);
		System.out.println("3 + 4 = " + (3 + 4));
		
		/*데이터 타입(Data type)
		 *변수의 선언 : 변수명 = 값
		 */
	}

}
