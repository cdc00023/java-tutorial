package com.example.hi;

public class P03 {

	public static void main(String[] args) {
		//over, under flow
		//오버플로우 : 범위초과
		//언더플로우 : 범위보다 낮음
		
		//byte  범위는 -128 ~ 127 사이의 정수
		byte num1 = 127;
		byte num2 = (byte)128;
		System.out.println("num1 = " +num1);
		System.out.println("num1  " +num2);
		
		
		
		
	}

}
